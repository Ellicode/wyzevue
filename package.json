{
  "name": "wyzevue",
  "version": "0.1.0",
  "description": "Vue component library",
  "main": "./dist/wyzevue.umd.js",
  "module": "./dist/wyzevue.es.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/wyzevue.es.js",
      "require": "./dist/wyzevue.umd.js"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue,.ts src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.5.1",
    "vite-plugin-vue-devtools": "^1.0.0",
    "vue-tsc": "^1.8.5",
    "eslint": "^8.46.0",
    "eslint-plugin-vue": "^9.16.1",
    "vitest": "^0.34.1"
  },
  "keywords": [
    "vue",
    "components",
    "ui"
  ],
  "author": "",
  "license": "MIT"
}
